{
  "version": 2,
  "builds": [
    {
      "src": "www.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["views/**", "public/**", "routes/**", "app.js"]
      }
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/www.js"
    }
  ],
  "functions": {
    "www.js": {
      "maxDuration": 10
    }
  }
}
